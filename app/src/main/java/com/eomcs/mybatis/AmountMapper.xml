  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="AmountMapper">
   
  <select id="selectAmount" resultType="amount">
    select 
     name,
     fowardprice,
     midfielderprice,
     defenderprice
    from soccer_amount
